{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": "off",
    "arrow-parens": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "space-before-blocks": "off",
    "semi": "off",
    "require-jsdoc": "off",
    "func-call-spacing": "off",
    "eol-last": "off",
    "no-trailing-spaces": "off",
    "no-unused-vars": "off",
    "space-before-function-paren": "off",
    "padded-blocks": "off",
    "spaced-comment": "off",
    "operator-linebreak": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
